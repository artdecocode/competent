{
  "name": "competent",
  "version": "1.7.0",
  "description": "Extracts, Renders And Exports For Dynamic Render JSX Components From Within HTML.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "compile": "depack-dev src/depack -o compile/competent.js -a -c -p -O 2018 -s --source_map_include_content",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md -n _competent",
    "doc-dev": "NODE_DEBUG=doc doc-dev -o README.md -n _competent",
    "b": "alamode src -o build -s",
    "d": "yarn-s d1 externs",
    "d1": "typal compile/index.js",
    "externs": "typal types/externs.js",
    "build": "yarn-s d b doc",
    "e": "alanode"
  },
  "files": [
    "compile",
    "src",
    "types/externs.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/competent.git"
  },
  "keywords": [
    "competent",
    "jsx",
    "components",
    "html",
    "render",
    "preact"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/competent/issues"
  },
  "homepage": "https://github.com/artdecocode/competent#readme",
  "devDependencies": {
    "@depack/render": "^1.3.0",
    "@externs/preact": "^1.2.0",
    "@rqt/aqt": "^1.3.0",
    "@wrote/read": "^1.0.4",
    "alamode": "^2.4.0",
    "documentary": "^1.30.3",
    "eslint-config-artdeco": "1.0.1",
    "mismatch": "^1.2.0",
    "restream": "^3.7.4",
    "rexml": "^2.1.0",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  },
  "srcDependencies": {
    "@depack/render": "^1.3.0",
    "restream": "^3.7.3",
    "rexml": "^2.1.0"
  }
}
